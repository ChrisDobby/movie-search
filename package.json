    "eslint": "4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jest": "^21.12.1",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "eslint-watch": "3.0.1",
    "expect": "1.20.2",
    "express": "4.15.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "identity-obj-proxy": "^3.0.0",
