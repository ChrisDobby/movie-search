    "rimraf": "2.6.1",
    "style-loader": "0.14.1",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "url-loader": "0.5.8",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "2.17.1",
    "webpack-visualizer-plugin": "^0.1.11"
