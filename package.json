{
  "name": "movie-search",
  "version": "0.0.1",
  "description": "Dev environment",
  "author": "Chris Dobson",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "build:prod": "webpack --config ./webpack.config.prod.js --hide-modules --progress --colors",
    "lint": "node_modules/.bin/esw webpack.config.* src tools --ext .js --ext .jsx",
    "test": "jest",
    "lint:watch": "yarn run lint --watch",
    "test:watch": "yarn run test --watch",
    "docs": "node_modules/.bin/jsdoc src -r -d docs"
  },
  "dependencies": {
    "colors": "1.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "3.0.0-beta.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-jest": "^22.2.2",
    "babel-loader": "6.4.1",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "6.23.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "6.24.0",
    "compression": "1.6.2",
